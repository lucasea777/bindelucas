  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment.line.fsp",
        "settings": {
          "foreground": "#3f8300",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.name.class.fsp",
        "settings": {
          // "foreground": "#AE81FF",
          "foreground": "#e4d6fd",
        }
      },
      {
        "scope": "entity.name.function.fsp",
        "settings": {
          "foreground": "#A6E22D",
        }
      },
      {
        "scope": "comment.line.fsp",
        "settings": {
          "foreground": "#F92672",
        }
      },
      {
        "scope": "keyword.operator.new.fsp",
        "settings": {
          "foreground": "#ED266A",
        }
      },
      {
        "scope": "keyword.type.fsp",
        "settings": {
          "foreground": "#ED266A",
        }
      }
    ]
  },
  // "spellright.language": [
  //   "es, en"
  // ],
